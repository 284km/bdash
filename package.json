{
  "name": "Bdash",
  "description": "A simple business intelligence application.",
  "private": true,
  "version": "1.2.1",
  "scripts": {
    "start": "electron app",
    "watch": "webpack --watch",
    "test": "yarn run test:unit",
    "test:unit": "electron-mocha \"test/unit/**/*.test.ts\"",
    "test:remote": "electron-mocha \"test/remote/**/*.test.ts\"",
    "test:integration": "yarn run compile:prod && mocha --exit test/integration/index.ts",
    "lint": "yarn run lint:eslint && yarn run lint:tsc && yarn run lint:prettier",
    "lint:eslint": "eslint --ext '.js,.jsx,.ts,.tsx' .",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --list-different ./**/*.{js,jsx,ts,tsx,css,json}",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,json}'",
    "publish": "yarn run compile:prod && electron-builder build --publish always",
    "compile:prod": "ts-node scripts/precompile.ts && webpack --env.NODE_ENV=production"
  },
  "dependencies": {
    "@google-cloud/bigquery": "0.7.0",
    "classnames": "2.2.5",
    "codemirror": "5.33.0",
    "csv-stringify": "2.0.1",
    "font-awesome": "4.7.0",
    "fs-extra": "5.0.0",
    "immup": "3.0.0",
    "js-yaml": "3.10.0",
    "lodash": "4.17.4",
    "markdown-table": "1.1.1",
    "moment": "2.20.1",
    "mysql2": "1.5.1",
    "pg": "7.4.1",
    "plotly.js": "1.32.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-micro-flyout": "1.0.1",
    "react-modal": "3.1.10",
    "react-select": "1.2.1",
    "sqlite3": "4.0.0",
    "td": "0.3.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.3",
    "@types/codemirror": "0.0.55",
    "@types/csv-stringify": "1.4.1",
    "@types/mocha": "2.2.46",
    "@types/node": "9.3.0",
    "@types/plotly.js": "1.28.10",
    "@types/react": "16.0.34",
    "@types/react-dom": "16.0.3",
    "@types/react-select": "1.1.0",
    "css-loader": "0.28.8",
    "electron": "1.8.2",
    "electron-builder": "20.5.1",
    "electron-mocha": "6.0.1",
    "eslint": "4.16.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-react": "7.5.1",
    "execa": "0.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "prettier": "1.10.2",
    "spectron": "3.7.2",
    "ts-loader": "3.2.0",
    "ts-node": "5.0.1",
    "typescript": "2.7.2",
    "typescript-eslint-parser": "14.0.0",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-node-externals": "1.6.0"
  },
  "license": "MIT"
}
